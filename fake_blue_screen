//Esse código só funciona para teclado US no windows
#include "DigiKeyboard.h"

#define KEY_TAB               0x2b     // Keyboard TAB
int maxCase = 3;                       // Maximum number of current used cases +1
int randNumber;
const int TIMEDELAY = 2000;           // The delay before allowing the program to loop again

void setup() {
  DigiKeyboard.sendKeyStroke(KEY_R, MOD_GUI_LEFT);
  DigiKeyboard.delay(600);
  DigiKeyboard.print("https://www.youtube.com/watch?v=6EnSuulqZSs");
  DigiKeyboard.sendKeyStroke(KEY_ENTER);
  DigiKeyboard.delay(3000);
  DigiKeyboard.sendKeyStroke(KEY_F);
}
  
void loop() {  
// This loop is left blank intentionally...
} 
